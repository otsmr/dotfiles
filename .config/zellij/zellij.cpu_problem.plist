<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>zellij.cpu_problem.plist</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/privat/.config/zellij/cpu_problem.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/com.custom.zellij_monitor.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.custom.zellij_monitor.out</string>
    <key>StartInterval</key>
    <integer>30</integer>
</dict>
</plist>

